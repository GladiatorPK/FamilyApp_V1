<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.mapado.familiyapp_v1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

    <activity
        android:name="de.mapado.familiyapp_v1.HomeScreen_Activity"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>


        <activity
            android:name=".Shuffle_Activity"
            android:label="@string/title_activity_shuffle"
            android:parentActivityName=".HomeScreen_Activity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="de.mapado.familiyapp_v1.HomeScreen_Activity" />
        </activity>

        <activity
            android:name=".Waehrungsrechner_Activity"
            android:label="@string/title_activity_Waerungsrechner"
            android:parentActivityName=".HomeScreen_Activity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="de.mapado.familiyapp_v1.HomeScreen_Activity" />
        </activity>

    </application>

</manifest>
